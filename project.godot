config_version=5

[application]

config/name="stakland"
run/main_scene="res://scenes/levels/test_level.tscn"
config/features=PackedStringArray("4.2")
boot_splash/show_image=false

[autoload]

Game="*res://scripts/game.gd"
Player="*res://scripts/player.gd"
SyncManager="*res://addons/godot-rollback-netcode/SyncManager.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/vsync/vsync_mode=0

[editor_plugins]

enabled=PackedStringArray("res://addons/zylann.hterrain/plugin.cfg")

[importer_defaults]

scene={
"_subresources": {},
"animation/fps": 30,
"animation/import": true,
"animation/remove_immutable_tracks": true,
